rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /users/{useID} {
      allow read: if request.auth.uid == useID;
      allow write: if request.auth.uid == useID;
      allow update: if request.auth.uid == useID;
      allow delete: if request.auth.uid == useID;
    }
  }
}